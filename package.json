{
  "name": "vladstudio-current-file-path",
  "displayName": "Current File Path",
  "description": "Shows the current file path in the status bar",
  "version": "1.0.0",
  "publisher": "vladstudio",
  "repository": {
    "type": "git",
    "url": "https://github.com/vladstudio/vscode-current-file-path.git"
  },
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "file path",
    "status bar",
    "copy path",
    "current file",
    "file location",
    "breadcrumb",
    "file explorer",
    "workspace",
    "relative path",
    "absolute path",
    "clipboard",
    "file navigation",
    "developer tools",
    "productivity"
  ],
  "main": "./out/extension.js",
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "currentFilePath.copyPath",
        "title": "Copy Current File Path"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}